{
    "name": "fnsc/laravel-google-calendar",
    "description": "Manage your events on your Google Calendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Fonseca",
            "email": "gabrieldfnsc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.00",
        "illuminate/console": "^10.00",
        "illuminate/config": "^10.00",
        "google/apiclient": "^v2.2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "leroy-merlin-br/coding-standard": "v3.1.0",
        "rector/rector": "^0.17",
        "phpstan/phpstan": "^1.10",
        "phpro/grumphp": "^v2.0.0",
        "mockery/mockery": "^1.6.2",
        "orchestra/testbench": "^v8.5"
    },
    "autoload": {
        "psr-4": {
            "LaravelGoogleCalendar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "LaravelGoogleCalendar\\": "tests/Unit/src"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    }
}
